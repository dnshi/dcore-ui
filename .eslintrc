{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "no-param-reassign": 0,
    "no-console": 0,
    "consistent-return": 0
  }
}
